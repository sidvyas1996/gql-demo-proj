// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "postgresql"
  url      =env("DATABASE_URL")
}

model Person {
  id        Int      @id @default(autoincrement())
  firstName String
  lastName  String
  email     String   @unique
  phone     String?
  dob       DateTime?
  gender    String
  
}

generator client {
  provider = "prisma-client-js"
}
